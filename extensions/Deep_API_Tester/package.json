{
    "name": "antigravity-deep-api-tester",
    "displayName": "Antigravity Deep API Tester",
    "description": "Deep investigation of Antigravity custom APIs",
    "version": "0.2.0",
    "publisher": "antigravity-research",
    "engines": {
        "vscode": "^1.80.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "deepApiTester.testCascade",
                "title": "Tester: Test Cascade APIs"
            },
            {
                "command": "deepApiTester.testStateSync",
                "title": "Tester: Test UnifiedStateSync"
            },
            {
                "command": "deepApiTester.testChatAccess",
                "title": "Tester: Test Chat Message Access"
            },
            {
                "command": "deepApiTester.exportFindings",
                "title": "Tester: Export All Findings"
            },
            {
                "command": "deepApiTester.deepCascadeTest",
                "title": "Tester: Deep Cascade Testing (Phase 1B)"
            },
            {
                "command": "deepApiTester.testTransferActiveChat",
                "title": "Tester: Test transferActiveChat (Phase 1C)"
            },
            {
                "command": "deepApiTester.testWebViewAccess",
                "title": "Tester: Test WebView Access"
            },
            {
                "command": "deepApiTester.runAllDeepTests",
                "title": "Tester: Run ALL Deep Tests (1B + 1C)"
            },
            {
                "command": "deepApiTester.startMonitoring",
                "title": "Tester: Start Event Monitoring"
            },
            {
                "command": "deepApiTester.stopMonitoring",
                "title": "Tester: Stop Event Monitoring"
            },
            {
                "command": "deepApiTester.showEventLog",
                "title": "Tester: Show Event Log"
            },
            {
                "command": "deepApiTester.exportFindings",
                "title": "Tester: Export Findings"
            },
            {
                "command": "deepApiTester.introspectCascade",
                "title": "Tester: Introspect Cascade"
            }
        ]
    }
}