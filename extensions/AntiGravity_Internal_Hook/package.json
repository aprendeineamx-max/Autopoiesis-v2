{
  "name": "antigravity-internal-hook",
  "displayName": "AntiGravity Internal Hook",
  "description": "Native hook for AntiGravity automation, replacing external bots.",
  "version": "1.0.4",
  "publisher": "antigravity",
  "repository": {
    "type": "git",
    "url": "https://github.com/learn-anything/antigravity"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "antigravity.dumpDefaults",
        "title": "Antigravity: Dump Default Keybindings"
      },
      {
        "command": "antigravity.resume",
        "title": "Antigravity: Resume Chat (Manual)"
      },
      {
        "command": "antigravity.test_links",
        "title": "Antigravity: Link Stress Test"
      },
      {
        "command": "antigravity.force_accept_loop",
        "title": "Antigravity: Force Accept Loop"
      },
      {
        "command": "antigravity.ghostAgent.toggle",
        "title": "ðŸ‘» Ghost Agent: Toggle On/Off"
      },
      {
        "command": "antigravity.ghostAgent.openGenesis",
        "title": "ðŸ‘» Ghost Agent: Open Genesis Studio"
      },
      {
        "command": "antigravity.ghostAgent.exportChat",
        "title": "ðŸ‘» Ghost Agent: Export Current Chat"
      },
      {
        "command": "antigravity.ghostAgent.showStatus",
        "title": "ðŸ‘» Ghost Agent: Show Status"
      },
      {
        "command": "antigravity.ghostAgent.configure",
        "title": "ðŸ‘» Ghost Agent: Configure Settings"
      },
      {
        "command": "antigravity.ghostAgent.openDashboard",
        "title": "ðŸ‘» Ghost Agent: Open Dashboard"
      }
    ],
    "keybindings": [
      {
        "command": "antigravity.ghostAgent.toggle",
        "key": "ctrl+alt+g",
        "mac": "cmd+alt+g"
      },
      {
        "command": "antigravity.ghostAgent.showStatus",
        "key": "ctrl+alt+s",
        "mac": "cmd+alt+s"
      }
    ]
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "antigravity",
    "automation"
  ],
  "author": "Ghost Agent",
  "license": "UNLICENSED"
}